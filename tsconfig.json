{
  "compilerOptions": {
	"target": "es2018",
	"module": "commonjs",
	"moduleResolution": "node",
	"noEmitOnError": true,
	"strict": true,
	"noImplicitReturns": true,
	"noFallthroughCasesInSwitch": true,
	"esModuleInterop": true,
	"resolveJsonModule": true,
	"outDir": "./dist"
  },
  "exclude": [
	"node_modules"
  ]
}